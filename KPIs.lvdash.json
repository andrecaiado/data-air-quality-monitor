{
  "datasets": [
    {
      "name": "abad16cd",
      "displayName": "sensor_daily_completeness",
      "queryLines": [
        "-- Country + parameter daily trend\n",
        "SELECT country_code, parameter_name, day_start, AVG(avg_value) AS country_daily_avg\n",
        "FROM airq.sensor_daily_completeness\n",
        "WHERE day_start >= date_sub(current_date, :lookback_days)\n",
        "GROUP BY country_code, parameter_name, day_start\n",
        "ORDER BY day_start;"
      ],
      "parameters": [
        {
          "displayName": "lookback_days",
          "keyword": "lookback_days",
          "dataType": "INTEGER",
          "defaultSelection": {
            "values": {
              "dataType": "INTEGER",
              "values": [
                {
                  "value": "30"
                }
              ]
            }
          }
        }
      ]
    }
  ],
