{
  "datasets": [
    {
      "name": "abad16cd",
      "displayName": "location_daily_trend",
      "queryLines": [
        "-- Location + parameter daily trend + max limit recommended\n",
        "WITH max_recommended AS (\n",
        "    SELECT *\n",
        "    FROM (\n",
        "        VALUES\n",
        "            (2, 20),         -- PM2.5 (24h)\n",
        "            (1, 45),         -- PM10 (24h)\n",
        "            (3, 120),        -- Ozone (8h)\n",
        "            (5, 50),         -- NO2 (24h)\n",
        "            (6, 50),         -- SO2 (24h)\n",
        "            (4, 4)           -- CO (24h) → 4 mg/m³ = 4000 µg/m³\n",
        "    ) AS t(parameter_id, max_limit)\n",
        ")\n",
        "SELECT sdc.country_code, location_id, loc.name as location_name, parameter_id, parameter_name, day_start, avg_value, mr.max_limit AS max_limit\n",
        "FROM airq.sensor_daily_completeness AS sdc\n",
        "LEFT JOIN airq.dim_locations loc USING (location_id)\n",
        "LEFT JOIN max_recommended mr USING (parameter_id)\n",
        "WHERE day_start >= date_sub(current_date, :lookback_days)\n",
        "GROUP BY sdc.country_code, location_id, loc.name, parameter_id, parameter_name, day_start, avg_value, max_limit\n",
        "ORDER BY day_start;"
      ],
      "parameters": [
        {
          "displayName": "lookback_days",
          "keyword": "lookback_days",
          "dataType": "INTEGER",
          "defaultSelection": {
            "values": {
              "dataType": "INTEGER",
              "values": [
                {
                  "value": "10"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "3365ea60",
      "displayName": "data_completeness",
      "queryLines": [
        "-- Data completeness\n",
        "SELECT\n",
        "  AVG(completeness_ratio) AS completeness_percent, loc.name as location_name\n",
        "FROM airq.sensor_daily_completeness\n",
        "LEFT JOIN airq.dim_locations loc USING (location_id)\n",
        "WHERE day_start >= date_sub(current_date(), :lookback_days)\n",
        "GROUP BY location_name\n",
        "ORDER BY completeness_percent DESC;"
      ],
      "parameters": [
        {
          "displayName": "lookback_days",
          "keyword": "lookback_days",
          "dataType": "INTEGER",
          "defaultSelection": {
            "values": {
              "dataType": "INTEGER",
              "values": [
                {
                  "value": "10"
                }
              ]
            }
          }
        }
      ],
      "catalog": "workspace",
      "schema": "airq"
    },
    {
      "name": "e963ee19",
      "displayName": "max_avg_values",
      "queryLines": [
        "SELECT parameter_id, parameter_name, avg_value AS max_avg_value, parameter_units, location_id, location_name\n",
        "FROM (\n",
        "    SELECT \n",
        "        sdc.parameter_id,\n",
        "        sdc.parameter_name,\n",
        "        sdc.avg_value,\n",
        "        s.parameter_units,\n",
        "        sdc.location_id,\n",
        "        loc.name as location_name,\n",
        "        ROW_NUMBER() OVER (\n",
        "            PARTITION BY sdc.parameter_name \n",
        "            ORDER BY sdc.avg_value DESC\n",
        "        ) AS rn\n",
        "    FROM workspace.airq.sensor_daily_completeness sdc\n",
        "    LEFT JOIN workspace.airq.dim_locations loc\n",
        "        ON sdc.location_id = loc.location_id\n",
        "    LEFT JOIN workspace.airq.dim_sensors s\n",
        "        ON sdc.sensor_id = s.sensor_id\n",
        "    WHERE sdc.day_start >= date_sub(current_date, :lookback_days)\n",
        ") t\n",
        "WHERE rn = 1\n",
        "ORDER BY parameter_name ASC;"
      ],
      "parameters": [
        {
          "displayName": "lookback_days",
          "keyword": "lookback_days",
          "dataType": "INTEGER",
          "defaultSelection": {
            "values": {
              "dataType": "INTEGER",
              "values": [
                {
                  "value": "10"
                }
              ]
            }
          }
        }
      ]
    }
  ],
